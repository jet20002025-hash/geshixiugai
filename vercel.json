{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "installCommand": "pip install -r requirements.txt",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "runtime": "python3.9"
    }
  }
}

